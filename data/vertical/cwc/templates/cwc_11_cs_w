NAME "Web corpus of Czech"
PATH "/opt/projects/lindat-services-kontext/devel/data/corpora/data/monolingual/CWC/2011/CWC_cs-w"
VERTICAL "/opt/projects/lindat-services-kontext/devel/data/corpora/vert/monolingual/CWC/2011/CWC_cs-w"
ENCODING utf-8
INFO "Web corpus of Czech"
LANGUAGE "Czech"

TAGSETDOC "http://ucnk.ff.cuni.cz/bonito/znacky.php"
WPOSLIST ",podstatné jméno,N.*,přídavné jméno,A.*,zájmeno,P.*,číslovka,C.*,sloveso,V.*,příslovce,D.*,předložka,R.*,spojka,J.*,částice,T.*,citoslovce,I.*,interpunkce,Z.*,neznámý,X.*"
DOCSTRUCTURE doc


ATTRIBUTE word {
        TYPE "FD_FGD"
        LOCALE "cs_CS.UTF-8"
}
ATTRIBUTE lemma {
        TYPE "FD_FGD"
        LOCALE "cs_CS.UTF-8"
}
ATTRIBUTE tag {
        TYPE "FD_FGD"
}
ATTRIBUTE afun {
        TYPE "FD_FGD"
}
ATTRIBUTE lc {
        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "cs_CS.UTF-8"
        FROMATTR word
        TYPE index
        TRANSQUERY yes
}
ATTRIBUTE pos {
        DYNAMIC getnchar
        DYNLIB  internal
        FUNTYPE i
        ARG1    1
        FROMATTR tag
        TYPE index
}
ATTRIBUTE   k {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    1
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}
ATTRIBUTE   g {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    3
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}
ATTRIBUTE   c {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    5
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}

STRUCTURE s {
}

STRUCTURE doc {
	 ATTRIBUTE id 
	ATTRIBUTE wordcount
}
MAXCONTEXT 50
MAXDETAIL 50

