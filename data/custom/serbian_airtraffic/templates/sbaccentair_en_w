NAME "English TTS speech corpus of air traffic (pilot) messages - Serbian accent - English - words only"
PATH "/opt/projects/lindat-services-kontext/devel/data/corpora/data/speech/Sbaccentair/Sbaccentair_en-w"
VERTICAL "/opt/projects/lindat-services-kontext/devel/data/corpora/vert/speech/Sbaccentair/Sbaccentair_en-w"
ENCODING utf-8
INFO "English TTS speech corpus of air traffic (pilot) messages - Serbian accent - - English - words only"
INFOHREF "https://lindat.mff.cuni.cz/repository/xmlui/handle/11234/1-1462"
LANGUAGE "English"

DOCSTRUCTURE doc

ATTRIBUTE word {
        TYPE "FD_FGD"
        LOCALE "en_US.UTF-8"
}
ATTRIBUTE lc {
        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "en_US.UTF-8"
        FROMATTR word
        TYPE index
        TRANSQUERY yes
}
STRUCTURE doc {
    ATTRIBUTE wordcount
}

STRUCTURE seg {
	ATTRIBUTE soundfile
}

MAXCONTEXT 50
MAXDETAIL 50

