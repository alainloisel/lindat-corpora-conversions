ENCODING utf-8
INFO "UMC 0.1 - Czech - POS tags only"
LANGUAGE "Czech"

TAGSETDOC "http://ucnk.ff.cuni.cz/bonito/znacky.php"
WPOSLIST ",podstatné jméno,N.*,přídavné jméno,A.*,zájmeno,P.*,číslovka,C.*,sloveso,V.*,příslovce,D.*,předložka,R.*,spojka,J.*,částice,T.*,citoslovce,I.*,interpunkce,Z.*,neznámý,X.*"
DOCSTRUCTURE doc

ALIGNED "umc_01_ru_m"
ALIGNSTRUCT align

ATTRIBUTE word {
        TYPE "FD_FGD"
        LOCALE "cs_CS.UTF-8"
}
ATTRIBUTE lemma {
        TYPE "FD_FGD"
        LOCALE "cs_CS.UTF-8"
}
ATTRIBUTE tag {
        TYPE "FD_FGD"
}
ATTRIBUTE lc {
        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "cs_CS.UTF-8"
        FROMATTR word
        TYPE index
        TRANSQUERY yes
}
ATTRIBUTE pos {
        DYNAMIC getnchar
        DYNLIB  internal
        FUNTYPE i
        ARG1    1
        FROMATTR tag
        TYPE index
}
ATTRIBUTE   k {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    1
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}
ATTRIBUTE   g {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    3
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}
ATTRIBUTE   c {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    5
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}

STRUCTURE doc {
	ATTRIBUTE id
    ATTRIBUTE wordcount
}

STRUCTURE align {
	ATTRIBUTE id
}

STRUCTURE s {
}

