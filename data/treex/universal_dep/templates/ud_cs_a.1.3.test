NAME "Universal Dependencies 1.3- Czech - TEST"
PATH "/opt/projects/lindat-services-kontext/devel/data/corpora/data/monolingual/UD/1.2/ud_cs-a"
VERTICAL "/opt/projects/lindat-services-kontext/devel/data/corpora/vert/monolingual/UD/1.2/cs-ud-train.vert"
ENCODING utf-8
INFO "The Czech UD treebank is based on the Prague Dependency Treebank 3.0 (PDT). The treebank consists of 87,913 sentences (1.5 M tokens) and its domain is mainly newswire, reaching also to business and popular scientific articles from the 1990s."
LANGUAGE "Czech"

TAGSETDOC "http://universaldependencies.github.io/docs/u/feat/index.html"
DOCSTRUCTURE doc

ATTRIBUTE word {
        TYPE "FD_FGD"
        LOCALE "cs_CZ.UTF-8"
}

ATTRIBUTE lc {
        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "cs_CZ.UTF-8"
        FROMATTR word
        TYPE index
        TRANSQUERY yes
}

ATTRIBUTE lemma {
        TYPE "FD_FGD"
        LOCALE "cs_CZ.UTF-8"
}
ATTRIBUTE pos {
        TYPE "FD_FGD"
}
ATTRIBUTE ufeat {
        TYPE "FD_FGD"
        
}

ATTRIBUTE ord {
        TYPE "FD_FGD"
} 

ATTRIBUTE deprel {
        TYPE "FD_FGD"
}

ATTRIBUTE p_word {
          TYPE "FD_FGD"
          LOCALE "cs_CZ.UTF-8"
}

ATTRIBUTE lc_p {
        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "cs_CZ.UTF-8"
        FROMATTR p_word
        TYPE index
        TRANSQUERY yes
}

ATTRIBUTE p_lemma {
          TYPE "FD_FGD"
          LOCALE "cs_CZ.UTF-8"
}
ATTRIBUTE p_pos {
          TYPE "FD_FGD"
}
ATTRIBUTE p_ufeat {
          TYPE "FD_FGD"
}

ATTRIBUTE p_deprel {
          TYPE "FD_FGD"
}

ATTRIBUTE p_ord {
          TYPE "FD_FGD"
}

ATTRIBUTE parent {
          TYPE "FD_FGD"
}

ATTRIBUTE children {
	  TYPE "FD_FGD"
          MULTIVALUE y
          MULTISEP "|"
}


STRUCTURE title {
    ATTRIBUTE name
}

STRUCTURE issue {
   ATTRIBUTE article_number
   ATTRIBUTE id
   ATTRIBUTE name
   ATTRIBUTE number
   ATTRIBUTE year
}

STRUCTURE s {
	ATTRIBUTE id
	ATTRIBUTE orig_file_sentence
}

STRUCTURE doc {
	ATTRIBUTE id
    ATTRIBUTE wordcount
}

MAXCONTEXT 50
MAXDETAIL 50

