NAME "HamleDT 2.0 - Portugese"
PATH "/opt/projects/lindat-services-kontext/production/data/corpora/data/monolingual/HamleDT/2.0/HamleDT_2.0_pt-a"
VERTICAL "/opt/projects/lindat-services-kontext/production/data/corpora/vert/monolingual/HamleDT/2.0/HamleDT_2.0_pt-a"
ENCODING utf-8
INFO "HamleDT 2.0 is a collection of 30 existing treebanks harmonized into a common annotation style, the Prague Dependencies. It is searchable for attributes POS and AFUN."
LANGUAGE "Portugese"

TAGSETDOC "https://ufal.mff.cuni.cz/pdt/Morphology_and_Tagging/Doc/hmptagqr.html"
WPOSLIST ",podstatné jméno,N.*,přídavné jméno,A.*,zájmeno,P.*,číslovka,C.*,sloveso,V.*,příslovce,D.*,předložka,R.*,spojka,J.*,částice,T.*,citoslovce,I.*,interpunkce,Z.*,neznámý,X.*"
DOCSTRUCTURE doc

ATTRIBUTE word {
        TYPE "FD_FGD"
        LOCALE "pt_PT.UTF-8"
}
ATTRIBUTE lemma {
        TYPE "FD_FGD"
        LOCALE "pt_PT.UTF-8"
}
ATTRIBUTE tag {
        TYPE "FD_FGD"
}
ATTRIBUTE afun {
        TYPE "FD_FGD"
}
ATTRIBUTE lc {
        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "pt_PT.UTF-8"
        FROMATTR word
        TYPE index
        TRANSQUERY yes
}
ATTRIBUTE pos {
        DYNAMIC getnchar
        DYNLIB  internal
        FUNTYPE i
        ARG1    1
        FROMATTR tag
        TYPE index
}
ATTRIBUTE   k {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    1
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}
ATTRIBUTE   g {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    3
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}
ATTRIBUTE   c {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    5
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}

STRUCTURE doc {
	ATTRIBUTE id
    ATTRIBUTE wordcount
}

STRUCTURE s {
	ATTRIBUTE id
}

MAXCONTEXT 50
MAXDETAIL 50

