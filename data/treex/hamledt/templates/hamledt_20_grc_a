NAME "HamleDT 2.0 - Ancient Greek - with POS tags and AFUN"
PATH "/opt/projects/lindat-services-kontext/devel/data/corpora/data/monolingual/HamleDT/2.0/HamleDT_2.0_grc-a"
VERTICAL "/opt/projects/lindat-services-kontext/devel/data/corpora/vert/monolingual/HamleDT/2.0/HamleDT_2.0_grc-a"
ENCODING utf-8
INFO "HamleDT 2.0 - Ancient Greek - with POS tags and AFUN"
LANGUAGE "Ancient Greek"
LOCALE "el_GR.UTF-8"

TAGSETDOC "http://ucnk.ff.cuni.cz/bonito/znacky.php"
WPOSLIST ",podstatné jméno,N.*,přídavné jméno,A.*,zájmeno,P.*,číslovka,C.*,sloveso,V.*,příslovce,D.*,předložka,R.*,spojka,J.*,částice,T.*,citoslovce,I.*,interpunkce,Z.*,neznámý,X.*"
DOCSTRUCTURE doc

ATTRIBUTE word {
        TYPE "FD_FGD"
        LOCALE "el_GR.UTF-8"
}
ATTRIBUTE lemma {
        TYPE "FD_FGD"
        LOCALE "el_GR.UTF-8"
}
ATTRIBUTE tag {
        TYPE "FD_FGD"
}
ATTRIBUTE afun {
        TYPE "FD_FGD"
}
ATTRIBUTE lc {
        DYNAMIC utf8lowercase
        DYNLIB internal
        FUNTYPE s
        ARG1 "el_GR.UTF-8"
        FROMATTR word
        TYPE index
        TRANSQUERY yes
}
ATTRIBUTE pos {
        DYNAMIC getnchar
        DYNLIB  internal
        FUNTYPE i
        ARG1    1
        FROMATTR tag
        TYPE index
}
ATTRIBUTE   k {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    1
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}
ATTRIBUTE   g {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    3
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}
ATTRIBUTE   c {
	DYNAMIC getnchar
	DYNLIB  internal
	ARG1    5
	FUNTYPE i
	FROMATTR tag
	TYPE 	index
}

STRUCTURE doc {
	ATTRIBUTE id
    ATTRIBUTE wordcount
}

STRUCTURE s {
	ATTRIBUTE id
}

MAXCONTEXT 50
MAXDETAIL 50

